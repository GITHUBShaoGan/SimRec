<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.slut.simrec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.v.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pswd.master.MasterTypeActivity" />
        <activity
            android:name=".pswd.master.grid.v.GridPassActivity"
            android:windowSoftInputMode="stateAlwaysVisible|adjustResize" />
        <activity
            android:name=".pswd.unlock.grid.v.GridUnlockActivity"
            android:windowSoftInputMode="stateAlwaysVisible|adjustResize" />
        <activity
            android:name=".pswd.create.PswdNewActivity"
            android:windowSoftInputMode="adjustResize|stateAlwaysVisible" />
        <activity android:name=".pswd.defaultcat.v.DefaultCatActivity"></activity>
    </application>

</manifest>